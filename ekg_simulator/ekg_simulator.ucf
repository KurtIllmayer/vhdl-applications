net "reset_i"      LOC = "A7";
NET "clock_i"  PERIOD = 20.0ns HIGH 50%; 
net "clock_i"      LOC = "B8";

NET "data_o<3>"    LOC = "B2"  | DRIVE = 8; # Signal name = JA1
NET "data_o<2>"    LOC = "A3"  | DRIVE = 8; # Signal name = JA2
NET "data_o<1>"    LOC = "J3"  | DRIVE = 8; # Signal name = JA3
NET "data_o<0>"    LOC = "B5"  | DRIVE = 8; # Signal name = JA4
NET "data_o<5>"    LOC = "C6"  | DRIVE = 8; # Signal name = JB1
NET "data_o<4>"    LOC = "B6"  | DRIVE = 8; # Signal name = JB2

#######################################################################
# Pin assignment for VGA
#######################################################################
NET "clk_i"  PERIOD = 10.0ns HIGH 50%; 
NET "clk_i"  LOC = "M6"; # Bank = 2, 4 PIN Crystal Oscillator

NET "h_sync_o"    LOC = "J14"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = HSYNC
NET "v_sync_o"    LOC = "K13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = VSYNC
NET "red_o<2>"    LOC = "F13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = RED2
NET "red_o<1>"    LOC = "D13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = RED1
NET "red_o<0>"    LOC = "C14"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = RED0
NET "green_o<2>"  LOC = "G14"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = GRN2
NET "green_o<1>"  LOC = "G13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = GRN1 
NET "green_o<0>"  LOC = "F14"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = GRN0 
NET "blue_o<1>"   LOC = "J13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = BLU2
NET "blue_o<0>"   LOC = "H13"  | DRIVE = 2  | PULLUP ; # Bank = 1, Signal name = BLU1